<odoo>
    <data>
        <record id="view_odoo14_attendee_tree" model="ir.ui.view">
            <field name="name">odoo14.attendee.tree</field>
            <field name="model">odoo14.attendee</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Attendee">
                    <field name="name"/>
                    <field name="session_id"/>
                    <field name="partner_id"/>
                </tree>
            </field>
        </record>

        <record id="view_odoo14_attendee_form" model="ir.ui.view">
            <field name="name">view_odoo14_attendee_form</field>
            <field name="model">odoo14.attendee</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Attendee">  
                    <group>
                        <field name="name"/>
                        <field name="session_id"/>
                        <field name="partner_id"/>        
                    </group>
                </form>
            </field>
        </record>
    </data>
</odoo>
